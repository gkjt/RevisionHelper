<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
	<rotate
		android:duration="300"
		android:fromDegrees="50"
		android:toDegrees="0"
		android:pivotX="50%"
		android:pivotY="50%"></rotate>
	<alpha
		android:duration="300"
		android:fromAlpha="0.0"
		android:toAlpha="1.0"
		android:interpolator="@android:anim/accelerate_interpolator"></alpha>
	<translate
		android:duration="300"
		android:fromXDelta="43%"
		android:fromYDelta="125%"
		android:toXDelta="0%"
		android:toYDelta="0%"
		android:interpolator="@android:anim/linear_interpolator" ></translate>
</set>